{
  "projects": [
    {
        "name": "note-server",
        "path": "/var/www/server",
        "type": "nodejs",
        "gitRepo": "git@jihulab.com:catnote/note-server.git",
        "gitBranch": "master",
        "buildCmd": "pwd && ls -l && npm install && cp -rf /var/www/config/server/config.prod.ts ./config/ && cp -rf /var/www/certs/* ./",
        "startCmd": "npm run stop2 && npm run start2",
        "healthCheck": {
            "url": "http://localhost:7002/ping/health",
            "timeout": 10
        }
    },
    {
        "name": "note-web",
        "path": "/var/www/note-web",
        "type": "nodejs",
        "gitRepo": "git@jihulab.com:catnote/note-web.git",
        "gitBranch": "master",
        "buildCmd": "",
        "startCmd": "pm2 restart frontend",
        "healthCheck": {
        "url": "http://localhost:3000/health",
        "timeout": 30
        }
    }
  ]
}